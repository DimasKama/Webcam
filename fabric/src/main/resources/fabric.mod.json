{
  "schemaVersion": 1,
  "id": "webcam",
  "version": "${version}",
  "name": "Webcam",
  "description": "Webcams in Minecraft",
  "authors": [
    "DimasKama"
  ],
  "contact": {
    "homepage": "https://modrinth.com/plugin/webcam-mod",
    "sources": "https://github.com/DimasKama/Webcam",
    "issues": "https://github.com/DimasKama/Webcam/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/webcam/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "ru.dimaskama.webcam.fabric.WebcamFabricPreLaunch"
    ],
    "main": [
      "ru.dimaskama.webcam.fabric.WebcamFabric"
    ],
    "client": [
      "ru.dimaskama.webcam.client.fabric.WebcamFabricClient"
    ],
    "modmenu": [
      "ru.dimaskama.webcam.client.fabric.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "webcam.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=${min_minecraft_version}",
    "java": ">=21",
    "fabric-api": "*"
  },
  "recommends": {
    "modmenu": "*",
    "cloth-config": "*"
  }
}
